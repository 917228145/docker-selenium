#!/usr/bin/env bash

echo "selenium_version=$(selenium_version) selenium_revision_version=$(selenium_revision_version)"

echo "chrome_stable_version=$(chrome_stable_version)"
google-chrome-stable --version

echo "firefox_version=$(firefox_version)"
firefox --version
echo "firefox-for-sel-2_version=$(firefox-for-sel-2_version)"
echo "firefox-for-sel-3_version=$(firefox-for-sel-3_version)"

echo "chromedriver_version=$(chromedriver_version)"
chromedriver --version

echo "geckodriver_version=$(geckodriver_version)"
/opt/geckodriver --version

echo "java_build_version=$(java_build_version)"
java -version

echo "python_version=$(python_version)"
python --version
