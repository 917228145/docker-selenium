#!/usr/bin/env bash
set -e

# Example:
#  firefox -version 2>&1
#=> Mozilla Firefox 49.0.1

# Example:
#=> 49.0.1
FFBIN="/home/seluser/firefox-for-sel-3/firefox"
if ${FFBIN} >/dev/null 2>&1; then
  ${FFBIN} -version 2>&1 | grep -Po '(?<=Firefox )([a-z0-9\.]+)'
else
  ${FFBIN} #fail and show error
fi
